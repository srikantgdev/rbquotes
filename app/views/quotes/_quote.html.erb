<%# app/views/quotes/_quote.html.erb %>

<%= turbo_frame_tag quote do %>
  <tr>
    <td width="85%"><%= quote.name %></td>
    <td>
      <%= link_to "Delete", quote_path(quote), class: "btn btn-sm btn-warning" %>
      <%= link_to "Edit", edit_quote_path(quote), class: "btn btn-sm btn-info" %>
    </td>
  </tr>
<% end %>